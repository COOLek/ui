  },
  "devDependencies": {
    "autoprefixer-core": "^5.2.1",
    "babel": "^5.8.23",
    "bh": "^4.1.3",
    "borschik-tech-cleancss": "^2.0.1",
    "bower": "^1.5.3",
    "conventional-changelog": "0.0.17",
    "enb": "^0.17.2",
    "enb-bem-docs": "^0.10.0",
    "enb-bem-examples": "^0.5.10",
    "enb-bem-specs": "dragn/enb-bem-specs",
    "enb-bem-techs": "^2.0.1",
    "enb-bem-tmpl-specs": "^0.13.2",
    "enb-bemxjst": "^2.0.2",
    "enb-bh": "^1.0.0",
    "enb-borschik": "^2.0.0",
    "enb-diverse-js": "~0.1.0",
    "enb-magic-platform": "~0.5.0",
    "enb-modules": "~0.2.0",
    "enb-postcss": "^0.1.1",
    "git-hooks": "^1.0.0",
    "istanbul": "^0.3.19",
    "jscs": "^2.0.0",
    "jscs-alfa": "git+ssh://git@git/man/jscs-alfa.git",
    "jshint": "^2.8.0",
    "jshint-groups": "^0.8.0",
    "mversion": "^1.10.0",
    "postcss": "^5.0.3",
    "postcss-custom-properties": "^5.0.0",
    "postcss-each": "^0.6.0",
    "postcss-nested": "^1.0.0",
    "postcss-url": "^4.0.0",
    "protein-kit": "git+ssh://git@git.theprotein.io:protein/kit.git"
  },
  "scripts": {
    "start": "enb server",
    "make": "magic make && npm run dist",
    "protein": "node protein.js",
    "build": "magic make",
    "deps": "bower i",
    "test": "npm run lint && npm run build && npm run specs",
    "lint": "jshint-groups && jscs .",
    "specs": "magic run specs",
    "clean-dist": "find *.blocks -iname '*.css' | grep -v 'post.css' |  xargs rm",
    "rm-examples": "rm -rf desktop.examples && rm -rf touch.examples",
    "dist": "npm run clean-dist && node distribute",
    "coverage": "ISTANBUL_COVERAGE=yes npm run specs && istanbul report --include coverage.json html text && istanbul check-coverage --include coverage.json",
    "release-patch": "mversion patch",
    "release-minor": "mversion minor",
    "release-major": "mversion major"
  },
  "engines": {
    "node": ">=0.10"
  }
}
