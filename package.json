{
  "private": true,
  "author": "Anton Winogradov <winogradovaa@gmail.com>",
  "name": "nyc-public",
  "description": "NYC Public Library of UI components",
  "version": "3.3.0",
  "dependencies": {
    "bem-environ": "1.4.0",
    "lodash": "^3.9.3",
    "vow": "^0.4.9",
    "vow-fs": "^0.3.4"
  },
  "devDependencies": {
    "bem": "~0.9.0",
    "bem-naming": "0.5.0",
    "borschik-tech-cleancss": "2.0.0",
    "bower": "1.3.12",
    "bower-npm-install": "~0.5.4",
    "conventional-changelog": "0.0.17",
    "enb": "0.15.0",
    "enb-bem-docs": "0.8.0",
    "enb-bem-examples": "0.5.9",
    "enb-bem-specs": "dragn/enb-bem-specs",
    "enb-bem-techs": "1.0.3",
    "enb-bem-tmpl-specs": "0.11.1",
    "enb-bemxjst": "1.3.4",
    "enb-borschik": "~1.5.0",
    "enb-diverse-js": "0.1.0",
    "enb-magic-platform": "0.5.0",
    "enb-modules": "0.2.0",
    "enb-stylus": "1.2.3",
    "gemini": "^0.9.5",
    "git-hooks": "~0.0.6",
    "http-server": "^0.7.2",
    "istanbul": "0.3.5",
    "jscs": "1.13.1",
    "jscs-alfa": "git+ssh://git@git/man/jscs-alfa.git",
    "jshint": "2.5.11",
    "jshint-groups": "0.6.2",
    "mversion": "^1.10.0",
    "stylus": "0.49.3",
    "svgo": "0.5.0"
  },
  "scripts": {
    "start": "enb server",
    "make": "enb make --no-cache",
    "build": "magic make",
    "deps": "bower i",
    "compiled": "find . -iname '*.styl' | xargs stylus",
    "test": "npm run lint && npm run build && npm run specs && npm run tmpls",
    "lint": "jshint-groups && jscs .",
    "specs": "magic run specs",
    "tmpls": "magic run tmpl-specs",
    "coverage": "ISTANBUL_COVERAGE=yes npm run specs && istanbul report --include coverage.json html text && istanbul check-coverage --include coverage.json",
    "release-patch": "mversion patch",
    "release-minor": "mversion minor",
    "release-major": "mversion major"
  },
  "engines": {
    "node": ">=0.10"
  }
}
