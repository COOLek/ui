{
  "private": true,
  "author": "Anton Winogradov <winogradovaa@gmail.com>",
  "name": "nyc-public",
  "description": "NYC Public Library of UI components",
  "version": "4.0.1",
  "dependencies": {
    "bem-environ": "1.4.0",
    "lodash": "^3.9.3",
    "vow": "^0.4.9",
    "vow-fs": "^0.3.4"
  },
  "devDependencies": {
    "autoprefixer-core": "~5.0.0",
    "bem-naming": "^0.5.1",
    "borschik-tech-cleancss": "2.0.0",
    "bower": "1.3.12",
    "conventional-changelog": "0.0.17",
    "enb": "^0.17.0",
    "enb-bem-docs": "~0.8.0",
    "enb-bem-examples": "^0.5.10",
    "enb-bem-specs": "dragn/enb-bem-specs",
    "enb-bem-techs": "~1.0.3",
    "enb-bem-tmpl-specs": "~0.11.1",
    "enb-bemxjst": "https://github.com/enb-bem/enb-bemxjst.git#v2.0.0-beta4",
    "enb-borschik": "~1.5.0",
    "enb-diverse-js": "~0.1.0",
    "enb-magic-platform": "~0.5.0",
    "enb-modules": "~0.2.0",
    "enb-postcss": "^0.1.1",
    "git-hooks": "~0.0.6",
    "istanbul": "0.3.5",
    "jscs": "^2.0.0",
    "jscs-alfa": "git+ssh://git@git/man/jscs-alfa.git",
    "jshint": "2.5.11",
    "jshint-groups": "0.6.2",
    "mversion": "^1.10.0",
    "postcss": "^4.1.13",
    "postcss-custom-properties": "^4.0.0",
    "postcss-each": "^0.3.0",
    "postcss-nested": "^0.3.2",
    "postcss-url": "^4.0.0",
    "svgo": "0.5.0"
  },
  "scripts": {
    "start": "enb server",
    "make": "magic make && npm run dist",
    "build": "magic make",
    "deps": "bower i",
    "test": "npm run lint && npm run build && npm run specs",
    "lint": "jshint-groups && jscs .",
    "specs": "magic run specs",
    "clean-dist": "find *.blocks -iname '*.css' | grep -v 'post.css' |  xargs rm",
    "rm-examples": "rm -rf desktop.examples && rm -rf touch.examples",
    "dist": "npm run clean-dist && node distribute",
    "coverage": "ISTANBUL_COVERAGE=yes npm run specs && istanbul report --include coverage.json html text && istanbul check-coverage --include coverage.json",
    "release-patch": "mversion patch",
    "release-minor": "mversion minor",
    "release-major": "mversion major"
  },
  "engines": {
    "node": ">=0.10"
  }
}
